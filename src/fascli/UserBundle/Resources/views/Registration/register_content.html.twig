<form class="form-horizontal well span9 " style="margin-left:15%" action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" class="fos_user_registration_register">
   
  <div class="control_group" style="margin-bottom:10px">
      <div class="control-label">
{{ form_errors(form.nom) }}
{{ form_label(form.nom, 'Nom'|trans) }}
      </div>
<div class="controls">
{{ form_widget(form.nom) }}
</div>
</div>
<div class="control_group" style="margin-bottom:10px">
    <div class="control-label">
{{ form_errors(form.prenoms) }}
{{ form_label(form.prenoms, 'Prenoms'|trans) }}
</div>
    <div class="controls input-xxlarge">
{{ form_widget(form.prenoms) }}
</div>
</div>
<div class="control_group" style="margin-bottom:10px">
    <div class="control-label">
{{ form_errors(form.datNaissance) }}
{{ form_label(form.datNaissance, 'Date de naissance'|trans) }}
</div>
    <div class="controls input-xxlarge">
{{ form_widget(form.datNaissance) }}
</div>
</div>

<div class="control_group" style="margin-bottom:10px">
    <div class="control-label">
{{ form_errors(form.cashpoint) }}
{{ form_label(form.cashpoint, 'Cash Point'|trans) }}
</div>
    <div class="controls input-xxlarge">
{{ form_widget(form.cashpoint) }}
</div>
</div>

      <div class="control_group" style="margin-bottom:10px">
    <div class="control-label">
{{ form_errors(form.username) }}
{{ form_label(form.username, 'Nom d\'utilisateur'|trans) }}
</div>
    <div class="controls">
{{ form_widget(form.username) }}
</div>
</div>
      <div class="control_group" style="margin-bottom:10px">
    <div class="control-label">
{{ form_errors(form.email) }}
{{ form_label(form.email, 'Adresse email'|trans) }}
</div>
    <div class="controls">
{{ form_widget(form.email) }}
</div>
</div>
<div class="control_group" style="margin-bottom:10px">
    <div class="control-label">
{{ form_errors(form.telephone) }}
{{ form_label(form.telephone, 'Numéro de téléphone'|trans) }}
</div>
    <div class="controls">
{{ form_widget(form.telephone) }}
</div>
</div>
      
      <div class="control_group">
    <div class="control-label">
{{ form_errors(form.plainPassword) }}
{{ form_label(form.plainPassword, 'Identification'|trans) }}
</div>
    <div class="controls">
{{ form_widget(form.plainPassword) }}
</div>
</div>
      
      <div class="control_group">
{{ form_rest(form) }}
<div > 
      <a class="btn btn-primary pull-left" href="{{ path('app') }}">
                    RETOUR A L'ACCUEIL
                </a>
    </div>
<div>
      <input type="submit" class='btn btn-primary pull-right' value="{{ 'registration.submit'|trans({}, 'FOSUserBundle') }}" />  
    </div>
    </div>
    
</form>