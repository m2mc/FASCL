<form class="form-horizontal " action="{{ path('client_create') }}" method="post" {{ form_enctype(form) }}>
<div class="control_group " style="margin-bottom:10px">
      <div class="control-label">
{{ form_errors(form.nom) }}
{{ form_label(form.nom, 'Nom'|trans) }}
      </div>
<div class="controls ui-widget" >
{{ form_widget(form.nom) }}
</div>
</div>  
<div class="control_group" style="margin-bottom:10px">
      <div class="control-label">
{{ form_errors(form.prenoms) }}
{{ form_label(form.prenoms, 'Prenoms'|trans) }}
      </div>
<div class="controls">
{{ form_widget(form.prenoms) }}
</div>
</div>   
<div class="control_group " style="margin-bottom:10px">
      <div class="control-label">
{{ form_errors(form.datnaissance) }}
{{ form_label(form.datnaissance, 'Date de naissance'|trans) }}
      </div>
<div class="controls">
{{ form_widget(form.datnaissance) }}
</div>
</div>  
<div class="control_group" style="margin-bottom:10px">
      <div class="control-label">
{{ form_errors(form.sexe) }}
{{ form_label(form.sexe, 'Sexe'|trans) }}
      </div>
<div class="controls">
{{ form_widget(form.sexe, { 'attr': {'class': 'inactive'} }) }}
</div>
</div> 
<div class="control_group" style="margin-bottom:10px">
      <div class="control-label">
{{ form_errors(form.nationalite) }}
{{ form_label(form.nationalite, 'Nationalite'|trans) }}
      </div>
<div class="controls">
{{ form_widget(form.nationalite, { 'attr': {'class': 'inactive'} }) }}
</div>
</div>  
<div class="control_group" style="margin-bottom:10px">
      <div class="control-label">
{{ form_errors(form.telephone) }}
{{ form_label(form.telephone, 'Telephone'|trans) }}
      </div>
<div class="controls">
{{ form_widget(form.telephone) }}
</div>
</div> 
<div class="control_group" style="margin-bottom:10px">
      <div class="control-label">
{{ form_errors(form.profession) }}
{{ form_label(form.profession, 'Profession'|trans) }}
      </div>
<div class="controls">
{{ form_widget(form.profession, { 'attr': {'class': 'inactive'} }) }}
</div>
</div>  
<div class="control_group" style="margin-bottom:10px">
      <div class="control-label">
{{ form_errors(form.adresse) }}
{{ form_label(form.adresse, 'Adresse'|trans) }}
      </div>
<div class="controls">
{{ form_widget(form.adresse, { 'attr': {'class': 'inactive'} }) }}
</div>
</div>
<div class="control_group" style="margin-bottom:10px">
      <div class="control-label">
{{ form_label(form.submit, 'Enregistrer'|trans) }}
      </div>
<div class="controls">
{{ form_widget(form.submit, { 'attr': {'class': 'btn btn-primary pull-right'} }) }}
</div>
</div>
<div class="control_group">
{{ form_rest(form) }}
</div>
</form>
